        <Column Name="UpdateIP" DataType="String" Description="更新地址" Model="False" />
        <Column Name="UpdateTime" DataType="DateTime" Description="更新时间" Model="False" />
        <Column Name="Remark" DataType="String" Length="500" Description="备注" />
