{
  "$schema": "http://json.schemastore.org/template",
  "author": "NewLife",
  "classifications": [ "NewLife", "Windows", "WinForms", "Desktop" ],
  "name": "NewLife WinForm",
  "description": "CS架构的客户端桌面应用，给用户提供便捷操作，可对接硬件",
  "identity": "NewLife.WinForm",
  "shortName": "nwinform",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "Zero.Desktop",
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "目标框架",
      "datatype": "choice",
      "choices": [
        { "choice": "net10.0", "description": ".NET 10 (LTS)" },
        { "choice": "net9.0", "description": ".NET 9 (STS)" },
        { "choice": "net8.0", "description": ".NET 8 (LTS)" }
      ],
      "defaultValue": "net10.0",
      "replaces": "net10.0"
    }
  },
  "preferNameDirectory": true
