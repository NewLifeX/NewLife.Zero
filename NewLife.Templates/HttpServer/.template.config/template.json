{
  "$schema": "http://json.schemastore.org/template",
  "author": "NewLife",
  "classifications": [ "NewLife", "Server" ],
  "name": "NewLife HttpServer",
  "description": "轻量级Http服务器，可用于追求简单Http服务的场景，例如暴露硬件控制为Http接口，如打印组件",
  "identity": "NewLife.HttpServer",
  "shortName": "httpserver",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "Zero.HttpServer",
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "目标框架",
      "datatype": "choice",
      "choices": [
        { "choice": "net10.0", "description": ".NET 10 (LTS)" },
        { "choice": "net9.0", "description": ".NET 9 (STS)" },
        { "choice": "net8.0", "description": ".NET 8 (LTS)" }
      ],
      "defaultValue": "net10.0",
      "replaces": "net10.0"
    }
  },
  "preferNameDirectory": true
}